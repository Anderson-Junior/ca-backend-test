      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CaBackendTest.Infrastructure\CaBackendTest.Infrastructure.csproj" />
    <ProjectReference Include="..\CaBackendTeste.Application\CaBackendTest.Application.csproj" />
    <ProjectReference Include="..\CaBackendTeste.Domain\CaBackendTest.Domain.csproj" />
  </ItemGroup>

</Project>
